<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<HBox alignment="CENTER" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.calc.controller.BonusPaneController">
   <children>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="BONUS" VBox.vgrow="ALWAYS">
               <font>
                  <Font size="11.0" />
               </font></Label>
            <TextField id="bonus-text-field" fx:id="bonusTextField" alignment="CENTER" promptText="enter bonus">
               <VBox.margin>
                  <Insets left="5.0" right="5.0" />
               </VBox.margin></TextField>
         </children>
         <HBox.margin>
            <Insets left="5.0" right="5.0" />
         </HBox.margin>
      </VBox>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="REWARD" VBox.vgrow="ALWAYS">
               <font>
                  <Font size="11.0" />
               </font></Label>
            <TextField id="payment-bonus-text-field" fx:id="paymentTextField" alignment="CENTER" promptText="enter payment">
               <VBox.margin>
                  <Insets left="5.0" right="5.0" />
               </VBox.margin></TextField>
         </children>
         <HBox.margin>
            <Insets left="5.0" right="5.0" />
         </HBox.margin>
      </VBox>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="1.9" VBox.vgrow="ALWAYS">
               <font>
                  <Font size="11.0" />
               </font></Label>
            <TextField id="profit19-text-field" fx:id="profit19TextField" alignment="CENTER" editable="false">
               <VBox.margin>
                  <Insets left="5.0" right="5.0" />
               </VBox.margin></TextField>
         </children>
         <HBox.margin>
            <Insets left="5.0" right="5.0" />
         </HBox.margin>
      </VBox>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="PROFIT (BONUS)" VBox.vgrow="ALWAYS">
               <font>
                  <Font size="11.0" />
               </font></Label>
            <TextField id="profit-text-field" fx:id="profitTextField" alignment="CENTER" editable="false">
               <VBox.margin>
                  <Insets left="5.0" right="5.0" />
               </VBox.margin></TextField>
         </children>
         <HBox.margin>
            <Insets left="5.0" right="5.0" />
         </HBox.margin>
      </VBox>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <Label alignment="CENTER" maxWidth="1.7976931348623157E308" text="REQUIRED POINTS" VBox.vgrow="ALWAYS">
               <font>
                  <Font size="11.0" />
               </font></Label>
            <TextField id="points-text-field" fx:id="pointsTextField" alignment="CENTER" promptText="to next level">
               <VBox.margin>
                  <Insets left="5.0" right="5.0" />
               </VBox.margin></TextField>
         </children>
         <HBox.margin>
            <Insets left="5.0" right="5.0" />
         </HBox.margin>
      </VBox>
   </children>
</HBox>
